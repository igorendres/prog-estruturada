{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "C/C++: gcc.exe build active file",
      "type": "cppbuild",
      "command": "C:\\TDM-GCC-64\\bin\\gcc.exe",
      "args": [
        "-Wall",
        "-Wextra",
        "-g3",
        "${file}",
        "-o",
        "${fileDirname}\\output\\${fileBasenameNoExtension}.exe"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task to compile the currently active C file."
    }
  ]
}